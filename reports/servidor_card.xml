<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="report_servidor">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="serv">
                <t t-call="web.external_layout">
                    <h1>Servidor:</h1>
                    <t t-esc="serv.modelo"/>
                    <t t-esc="serv.marca"/>
                    <t t-esc="serv.tipoServidor"/>
                    <t t-esc="serv.precioAlquiler"/>
                    <t t-esc="serv.procesadores"/>
                    <t t-esc="serv.ram"/>
                    <t t-esc="serv.discosDuros"/>
                </t>
            </t>
        </t>
    </template>
</odoo>